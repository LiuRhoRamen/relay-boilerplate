{
  "private": true,
  "scripts": {
    "start": "npm run relay && cross-env webpack-dev-server --hot --inline",
    "build": "npm run update-schema && npm run relay && npm run webpack",
    "update-schema": "npm run server-build && babel-node ./server-compiled/scripts/updateSchema.js",
    "relay": "relay-compiler --src ./client/js --schema ./server-compiled/data/schema.graphql --extensions js jsx",
    "webpack": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "server-build": "babel server --out-dir server-compiled",
    "server": "npm run update-schema && nodemon --watch server/**/*.js --exec \"npm run update-schema && babel-node ./server-compiled\"",
    "clean": "rm -rf ./client/build && mkdir ./client/build"
  },
  "prettier": {},
  "dependencies": {
    "antd": "^3.2.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "dataloader": "^1.4.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.12",
    "file-loader": "^1.1.10",
    "found": "^0.3.8",
    "found-relay": "next",
    "graphql": "0.11.0",
    "graphql-relay": "0.5.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-relay": "^1.5.0",
    "url-loader": "^0.6.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-relay": "^1.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.4.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.6",
    "nodemon": "^1.17.1",
    "postcss-loader": "^2.1.1",
    "prettier": "1.10.2",
    "relay-compiler": "^1.5.0",
    "relay-devtools": "^1.4.0",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
